import time
import sys
time_start = time.time()
seconds = 0
minutes = 0
running = True
clear_output()

while running:
  try:
    sys.stdout.write("\r{minutes} Minutes {seconds} Seconds".format(minutes=minutes, seconds=seconds))
    sys.stdout.flush()
    time.sleep(1)
    seconds = int(time.time() - time_start) - minutes * 60
    if seconds >= 60:
      minutes += 1
      seconds = 0
  except KeyboardInterrupt as e:
    running = False
